void setup () {
  size (400,400);
  background (255);
}
float i = 0;
void draw() {
  if (i < height) {
    fill(random(255), random(255), random(255));
    stroke(0);
    ellipse(mouseY, mouseX, 7,7);
     stroke(random(width), random(width), random(width), random(width));
    line (random(width), 0, random(width), 400);
   
  }
}
